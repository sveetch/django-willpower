{
    "Blog": {
        "readonly_fields": ["created"],
        "search_fields": ["title"],
        "fields": {
            "title": {
                "kind": "CharField",
                "required": true
            },
            "created": {
                "kind": "DateTimeField",
                "auto_creation": true
            }
        }
    },
    "Article": {
        "autocompleted_fields": ["blog"],
        "fields": {
            "blog": {
                "kind": "ForeignKey",
                "required": true,
                "nullable": false,
                "default": null,
                "target": "{appname}.Blog",
                "related_name": "article",
                "on_delete": "models.CASCADE"
            },
            "title": {
                "kind": "CharField",
                "required": true
            },
            "content": {
                "kind": "TextField"
            }
        }
    }
}
