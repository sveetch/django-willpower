{
    "Blog": {
        "readonly_fields": ["created"],
        "search_fields": ["title"],
        "admin_list_display": ["title", "created"],
        "prepopulated_fields": {"slug": ["title"]},
        "list_filter": ["created"],
        "fields": {
            "title": {
                "kind": "CharField",
                "required": true
            },
            "slug": {
                "kind": "SlugField",
                "unique": true,
                "required": true
            },
            "created": {
                "kind": "DateTimeField",
                "auto_creation": true
            },
            "cover": {
                "kind": "ImageField"
            },
            "description": {
                "kind": "TextField"
            }
        }
    },
    "Article": {
        "fields": {
            "blogs": {
                "kind": "ManyToManyField",
                "required": true,
                "nullable": true,
                "default": null,
                "target": "star_biz.Blog",
                "related_name": "article",
                "on_delete": "models.CASCADE"
            },
            "title": {
                "kind": "CharField",
                "required": true
            },
            "slug": {
                "kind": "SlugField",
                "unique": true,
                "required": true
            },
            "created": {
                "kind": "DateTimeField",
                "auto_creation": true
            },
            "modified": {
                "kind": "DateTimeField",
                "auto_update": true
            },
            "attachment": {
                "kind": "FileField"
            },
            "cover": {
                "kind": "ImageField"
            },
            "content": {
                "kind": "TextField"
            },
            "published": {
                "kind": "BooleanField",
                "required": true
            }
        }
    },
    "Comment": {
        "default_order": ["title"],
        "provide_inline": true,
        "fields": {
            "article": {
                "kind": "ForeignKey",
                "required": true,
                "nullable": true,
                "default": null,
                "target": "star_biz.Article",
                "related_name": "comment",
                "on_delete": "models.CASCADE"
            },
            "blog": {
                "kind": "ForeignKey",
                "label": "related weblog",
                "required": false,
                "nullable": true,
                "default": null,
                "target": "star_biz.Blog"
            },
            "title": {
                "kind": "CharField",
                "required": true,
                "default": "coucou"
            },
            "created": {
                "kind": "DateTimeField",
                "auto_creation": true
            },
            "modified": {
                "kind": "DateTimeField",
                "auto_update": true
            },
            "published": {
                "kind": "DateTimeField",
                "nullable": true
            },
            "reported": {
                "kind": "DateField"
            },
            "email": {
                "kind": "EmailField",
                "default": null,
                "nullable": true,
                "required": true
            },
            "comment": {
                "kind": "TextField",
                "required": true
            },
            "rgpd": {
                "kind": "BooleanField",
                "required": true,
                "default": true
            },
            "counter": {
                "kind": "IntegerField",
                "default": 0,
                "min_value": 2,
                "max_value": 42
            }
        }
    }
}
